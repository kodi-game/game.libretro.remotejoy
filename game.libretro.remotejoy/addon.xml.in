<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.remotejoy"
		name="PSP RemotePlay (RemoteJoy)"
		version="1.0.0.1"
		provider-name="">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
